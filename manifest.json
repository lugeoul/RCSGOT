{
  "manifest_version": 2,

  "name": "RCSGOT",
  "description": "Reddit CSGO tradehelper",
  "version": "0.0",

  "permissions": [
    "http://www.reddit.com/*",
    "http://steamcommunity.com/*",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "html/popup.html"
  }
}